    "build": "next build",
    "start": "next start",
    "lint": "next lint src",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taga1117/claude-code-report.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taga1117/claude-code-report/issues"
  },
  "homepage": "https://github.com/taga1117/claude-code-report#readme",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@prisma/client": "^5.22.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.17",
